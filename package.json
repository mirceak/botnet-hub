{
  "version": "0.0.0",
  "type": "module",
  "bin": "build/main.js",
  "name": "app",
  "scripts": {
    "docs": "typedoc --entryPointStrategy expand ./ --exclude \"**/*+(.+(test|d)).ts\" --exclude \"node_modules/**\" --exclude \"docs/**\" --exclude \"build/**\" --exclude \"src/exampleTest/**\" --exclude \"coverage/**\" --exclude \"./jest.config.mjs\"",
    "dev": "nodemon -e * --watch src --exec \"yarn tsc-build && node --experimental-vm-modules build/main.js\"",
    "tsc-build": "set NODE_OPTIONS=es-module-specifier-resolution=node && tsc -p tsconfig.json && tsc-alias",
    "build:release": "set NODE_OPTIONS=es-module-specifier-resolution=node && tsc -p tsconfig.release.json && tsc-alias ",
    "test": "set NODE_OPTIONS=es-module-specifier-resolution=node && set NODE_OPTIONS=--experimental-vm-modules && jest --coverage",
    "test:watch": "set NODE_OPTIONS=--experimental-vm-modules && jest --watchAll --coverage",
    "prettier": "prettier --config .prettierrc.json --write .",
    "lint": "eslint",
    "lint-scss": "stylelint",
    "prepare": "node .husky/remove-old.js && git config --unset core.hooksPath && husky install && npx husky add .husky/pre-commit \"npx lint-staged\" && git add .husky/pre-commit"
  },
  "lint-staged": {
    "*.ts": "yarn lint --fix",
    "*.scss": "yarn lint-scss --fix"
  },
  "dependencies": {
    "jsdom": "^21.1.0",
    "sequelize": "^6.29.0",
    "sqlite3": "^5.1.4",
    "express": "^5.0.0-beta.1",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "sass": "^1.58.3",
    "jest": "^29.4.3"
  },
  "devDependencies": {
    "lint-staged": "^13.1.2",
    "path-to-regexp": "^6.2.1",
    "prettier": "^2.8.4",
    "eslint": "^8.35.0",
    "stylelint": "^15.2.0",
    "nodemon": "^2.0.20",
    "husky": "^8.0.3",
    "typescript": "^4.9.5",
    "typedoc": "^0.23.26",
    "tsc-alias": "^1.8.2",
    "ts-jest": "^29.0.5",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsdom": "^21.1.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.2",
    "@types/sequelize": "^4.28.14",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-selector-bem-pattern": "^2.1.1"
  }
}

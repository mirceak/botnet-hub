{
  "version": "0.0.0",
  "type": "module",
  "bin": "build/main.js",
  "name": "app",
  "scripts": {
    "docs": "typedoc --entryPointStrategy expand ./ --exclude \"**/*+(.+(test|d)).ts\" --exclude \"node_modules/**\" --exclude \"docs/**\" --exclude \"build/**\" --exclude \"src/exampleTest/**\" --exclude \"coverage/**\" --exclude \"./jest.config.mjs\"",
    "start": "nodemon --experimental-vm-modules ./build/main.js",
    "tsc-alias-watch": "yarn tsc-build && tsc -p tsconfig.json && tsc-alias -w",
    "tsc-build": "set NODE_OPTIONS=es-module-specifier-resolution=node && tsc -p tsconfig.json",
    "tsc-watch": "yarn tsc-build -w",
    "build:watch": "concurrently --kill-others \"yarn tsc-alias-watch\" \"yarn tsc-watch\" ",
    "build:release": "tsc -p tsconfig.release.json",
    "test": "set NODE_OPTIONS=es-module-specifier-resolution=node && set NODE_OPTIONS=--experimental-vm-modules && jest --coverage",
    "lint": "eslint . --ext .ts --ext .mts",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "set NODE_OPTIONS=--experimental-vm-modules && jest --watchAll --coverage",
    "husky:pre:commit": "yarn prettier && yarn tsc-build && yarn lint",
    "preinstall": "husky install"
  },
  "dependencies": {
    "express": "^5.0.0-beta.1",
    "sequelize": "^6.25.5",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.5",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "~2.7.1",
    "ts-jest": "^29.0.3",
    "tsc-alias": "^1.7.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  }
}

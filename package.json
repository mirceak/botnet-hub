{
  "version": "0.0.0",
  "type": "module",
  "bin": "build/main.js",
  "name": "app",
  "scripts": {
    "docs": "typedoc --entryPointStrategy expand ./ --exclude \"**/*+(.+(test|d)).ts\" --exclude \"node_modules/**\" --exclude \"docs/**\" --exclude \"build/**\" --exclude \"src/exampleTest/**\" --exclude \"coverage/**\" --exclude \"./jest.config.mjs\"",
    "start": "nodemon -e * --experimental-vm-modules --watch build/**/* --watch src/database/seed/remote/modules/utils/assets/**/* build/main.js",
    "dev": "nodemon -e * --watch src --exec \"yarn tsc-build && node --experimental-vm-modules build/main.js\"",
    "tsc-build": "set NODE_OPTIONS=es-module-specifier-resolution=node && tsc -p tsconfig.json && tsc-alias",
    "build:release": "set NODE_OPTIONS=es-module-specifier-resolution=node && tsc -p tsconfig.release.json && tsc-alias ",
    "test": "set NODE_OPTIONS=es-module-specifier-resolution=node && set NODE_OPTIONS=--experimental-vm-modules && jest --coverage",
    "test:watch": "set NODE_OPTIONS=--experimental-vm-modules && jest --watchAll --coverage",
    "prettier": "prettier --config .prettierrc.json --write .",
    "lint": "eslint . --ext .ts --ext .mts",
    "husky:staged:pretify": "yarn prettier",
    "husky:staged:lint": "yarn prettier && yarn tsc-build && yarn lint",
    "husky:full:manual": "yarn husky:staged:pretify & yarn husky:staged:lint",
    "postinstall": "husky install & npx husky add .husky/husky-staged-pretify \"yarn husky-staged-pretify\" & npx husky add .husky/husky-staged-lint \"yarn husky-staged-lint\""
  },
  "dependencies": {
    "tsc-alias": "^1.8.2",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "jsdom": "^21.0.0",
    "sequelize": "^6.28.0",
    "express": "^5.0.0-beta.1",
    "compression": "^1.7.4",
    "path-to-regexp": "^6.2.1",
    "prettier": "^2.8.2",
    "cors": "^2.8.5",
    "morgan": "^1.9.1",
    "sqlite3": "^5.1.4",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "ts-jest": "^29.0.5",
    "jest": "^29.3.1",
    "husky": "^8.0.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/jsdom": "^20.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/sequelize": "^4.28.14",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20"
  }
}
